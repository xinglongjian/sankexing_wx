page{
    -webkit-user-select: none;
    user-select: none;
    width: 100%;
    height: 100%;
    background-color: #f8f8f8;
    font-family: Arial, Helvetica, sans-serif;
    overflow-x: hidden;
    font-size: 32rpx;
    letter-spacing: 5rpx;
}

.flex{
    display: flex;
    justify-content: center;
    align-items: center;
}
.container {
  height: auto;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding: 0rpx 0;
  box-sizing: border-box;
  background-color: #EDEDED;
} 
.nodata{
  height: 100%;
  width: 100%;
  color: #EDEDED;
  text-align: center;
  position: absolute;
  top:50%;
}
.space10 {
  width: 100%;
  height: 10px;
  background-color: #EDEDED;
}