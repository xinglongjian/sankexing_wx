/* pages/profile/childs/index.wxss */

.header, .item {
  width: 100%;
  height: 40px;
  display: flex;
  flex-direction: row;
  background-color: #fff;
  justify-content: space-around;
  align-items: center;
}

.body {
  background-color: #fff;
}

.header text {
  font-size: 14px;
  border-right: 1px solid #fff;
}

.headimage {
  width: 36px;
  height: 36px;
  border-radius: 50%;
}

.item {
  padding-bottom: 30rpx;
  padding-top: 30rpx;
}

.footer {
  position: absolute;
  bottom: 0px;
  right: 0px;
  width: 100%;
}

.footer button {
  background-color: #1aad19;
}
