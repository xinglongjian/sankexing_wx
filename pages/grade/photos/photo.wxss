/* pages/grade/photo/photo.wxss */
page{ 
 display: flex; 
 flex-direction: column; 
 height: 100%; 
} 
.navbar{ 
 flex: none; 
 display: flex; 
 background: #fff; 
} 
.navbar .item{ 
 position: relative; 
 flex: auto; 
 text-align: center; 
 line-height: 100rpx; 
} 
.navbar .item.active{ 
 color: #000; 
} 
.navbar .item.active:after{ 
 content: ""; 
 display: block; 
 position: absolute; 
 bottom: 0; 
 left: 0; 
 right: 0; 
 height: 5rpx; 
 background: #1296db; 
} 

.tabpage {
   background: #fff;
   margin-top: 10px;
   height: 100%;
}

.upload {
  width: 50px;
  height: 50px;
  border-radius: 50%;
  position: absolute;
  top: 60px;
  right: 10px;

}
.upload image{
  width: 40px;
  height: 40px;
}